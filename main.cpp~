#include <iostream>

using namespace std;

int main() {
  cout << "X1: ";
  int x1;
  cin >> x1;
  cout << "Y1: ";
  int y1;
  cin >> y1;
  cout << "X2: ";
  int x2;
  cin >> x2;
  cout << "Y2: ";
  int y2;
  cin >> y2;
  cout << "X3: ";
  int x3;
  cin >> x3;
  cout << "y3: ";
  int y3;
  cin >> y3;

  return 0;
}
